{
  "name": "@frontmeans/render-scheduler",
  "version": "1.8.2",
  "description": "Schedule DOM updates rendering",
  "keywords": [
    "performance",
    "requestAnimationFrame",
    "raf"
  ],
  "homepage": "https://github.com/frontmeans/render-scheduler",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:frontmeans/render-scheduler.git"
  },
  "license": "MIT",
  "author": "Ruslan Lopatin <ruslan.lopatin@gmail.com>",
  "bugs": {
    "url": "https://github.com/frontmeans/render-scheduler/issues"
  },
  "type": "module",
  "types": "./dist/render-scheduler.d.ts",
  "exports": {
    "types": "./dist/render-scheduler.d.ts",
    "default": "./dist/render-scheduler.js"
  },
  "sideEffects": false,
  "dependencies": {
    "@frontmeans/dom-primitives": "^1.1.1",
    "@proc7ts/context-values": "7.1.1",
    "@proc7ts/supply": "^1.2.3"
  },
  "devDependencies": {
    "@jest/globals": "^29.6.4",
    "@proc7ts/context-builder": "^7.0.2",
    "@run-z/eslint-config": "^3.5.0",
    "@run-z/prettier-config": "^2.0.0",
    "@run-z/project-config": "^0.20.0",
    "@swc/core": "^1.3.80",
    "@swc/jest": "^0.2.29",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.48.0",
    "eslint-plugin-jest": "^27.2.3",
    "gh-pages": "^6.0.0",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "jest-junit": "^16.0.0",
    "jest-mock": "^29.6.3",
    "prettier": "^2.8.8",
    "prettier-eslint-cli": "^7.1.0",
    "rollup": "^3.28.1",
    "run-z": "^1.11.0",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "typedoc": "^0.25.0",
    "typescript": "~5.2.2"
  },
  "scripts": {
    "all": "run-z build,lint,test",
    "build": "run-z +z --then build-z",
    "ci:all": "run-z all +test/--ci/--runInBand",
    "clean": "run-z +z --then clean-z",
    "doc": "run-z +z --then typedoc",
    "doc:publish": "run-z doc --then gh-pages --dist target/typedoc --dotfiles",
    "format": "run-z +z --then prettier-eslint --write --include-dot-files \"src/**/*.*\" \"*.{js,cjs,json,md}\"",
    "lint": "run-z +z --then eslint .",
    "test": "run-z +z env:NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" --then test-z",
    "z": "run-z +cmd:build-z,+cmd:typedoc,+cmd:eslint,+cmd:test-z"
  }
}
