  "description": "Schedule DOM updates rendering",
  "keywords": [
    "performance",
    "requestAnimationFrame",
    "raf"
  ],
  "repository": "git+https://github.com/surol/render-scheduler.git",
  "license": "MIT",
  "author": "Ruslan Lopatin <ruslan.lopatin@gmail.com>",
  "main": "dist/render-scheduler.js",
  "module": "dist/render-scheduler.mjs",
  "typings": "index.d.ts",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@surol/eslint-config": "^1.0.3",
    "@types/jest": "^25.1.3",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "codecov": "^3.6.5",
    "eslint": "^6.8.0",
    "eslint-plugin-jest": "^23.7.0",
    "fs-extra": "^8.1.0",
    "gh-pages": "^2.1.1",
    "jest": "^25.1.0",
    "jest-junit": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.31.1",
    "rollup-plugin-sourcemaps": "^0.5.0",
    "rollup-plugin-typescript2": "^0.26.0",
    "ts-jest": "^25.2.1",
    "typedoc": "^0.16.10",
    "typedoc-plugin-external-module-name": "^3.0.0",
    "typescript": "^3.8.2"
  },
  "scripts": {
    "all": "run-p build:all test",
    "build": "rollup --config ./rollup.config.js",
    "build:all": "run-p rebuild lint",
    "ci:all": "run-p build:all ci:test",
    "ci:test": "jest --ci --runInBand",
    "clean": "node ./build/clean",
    "doc": "typedoc",
    "doc:gh-pages": "gh-pages --dist target/typedoc --dotfiles",
    "doc:ci": "node ./build/doc-ci",
    "doc:publish": "run-s doc doc:ci \"doc:gh-pages {@}\" --",
    "lint": "eslint . --ext .js,.ts",
    "rebuild": "run-s clean build",
    "test": "jest"
  }
